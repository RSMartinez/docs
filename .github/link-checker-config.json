{
    "ignorePatterns": [
        {
            "pattern": "^(http|https)://localhost"
        },
        {
            "pattern": "mailto:{{.*}}"
        },
        {
            "pattern": "https://github.com/{{.*}}/actions/runs/{{.*}}"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/",
            "replacement": "{{BASEURL}}/"
        }
    ],
    "retryOn429": true,
    "aliveStatusCodes": [
        200,
        206
    ]
}